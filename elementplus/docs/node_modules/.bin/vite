#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules/.pnpm/vite@2.9.5_sass@1.50.0/node_modules/vite/bin/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules/.pnpm/vite@2.9.5_sass@1.50.0/node_modules/vite/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules/.pnpm/vite@2.9.5_sass@1.50.0/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules/.pnpm/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules:/Users/luoxi/Downloads/1-培训准备文档/node_modules:/Users/luoxi/Downloads/node_modules:/Users/luoxi/node_modules:/Users/node_modules:/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/docs/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules/.pnpm/vite@2.9.5_sass@1.50.0/node_modules/vite/bin/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules/.pnpm/vite@2.9.5_sass@1.50.0/node_modules/vite/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules/.pnpm/vite@2.9.5_sass@1.50.0/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules/.pnpm/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/node_modules:/Users/luoxi/Downloads/1-培训准备文档/node_modules:/Users/luoxi/Downloads/node_modules:/Users/luoxi/node_modules:/Users/node_modules:/node_modules:/Users/luoxi/Downloads/1-培训准备文档/element-plus-dev/docs/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@2.9.5_sass@1.50.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@2.9.5_sass@1.50.0/node_modules/vite/bin/vite.js" "$@"
fi
